{
    "name": "khazl/timer-laravel",
    "description": "Laravel package to work easy and reliable with timers / countdowns.",
    "license": "MIT",
    "authors": [
        {
            "name": "Guido Gasper"
        }
    ],
    "homepage": "https://github.com/khazl/timer",
    "keywords": ["Laravel", "Timer", "Countdown", "Stop Watch"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Khazl\\Timer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Khazl\\Timer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Khazl\\Timer\\TimerServiceProvider"
            ],
            "aliases": {
                "Timer": "Discord"
            }
        }
    }
}
